@model IEnumerable<UserNameAndEmailViewModel>

<h2>Список пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-email="@user.Email">Права доступа</a>
            </td>
        </tr>
    }
</table>